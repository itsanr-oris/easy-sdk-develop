{
    "name": "f-oris/easy-sdk-develop",
    "description": "A library for easy sdk develop.",
    "license": "MIT",
    "authors": [
        {
            "name": "F.oris",
            "email": "us@f-oris.me"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-json": "*",
        "f-oris/easy-sdk-framework": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0",
        "mockery/mockery": "^0.9",
        "mikey179/vfsstream": "^1.6",
        "symfony/var-dumper": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Foris\\Easy\\Sdk\\Develop\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Foris\\Easy\\Sdk\\Develop\\Tests\\": "tests/"
        }
    },
    "extra": {
        "easy-sdk": {
            "providers": [
                "Foris\\Easy\\Sdk\\Develop\\ServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
